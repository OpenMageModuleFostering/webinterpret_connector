<config>
    <modules>
        <Webinterpret_Connector>
            <version>1.3.6.0</version>
        </Webinterpret_Connector>
    </modules>
    <global>
        <models>
            <webinterpret_connector>
                <class>Webinterpret_Connector_Model</class>
            </webinterpret_connector>
        </models>
        <blocks>
            <webinterpret_connector>
                <class>Webinterpret_Connector_Block</class>
            </webinterpret_connector>
        </blocks>
        <helpers>
            <webinterpret_connector>
                <class>Webinterpret_Connector_Helper</class>
            </webinterpret_connector>
        </helpers>
        <events>
            <admin_system_config_changed_section_webinterpret_connector>
                <observers>
                    <webinterpret_connector_admin_system_config_changed_section_webinterpret_connector>
                        <type>singleton</type>
                        <class>Webinterpret_Connector_Model_Observer</class>
                        <method>hookIntoAdminSystemConfigChangedSectionWebinterpretConnector</method>
                    </webinterpret_connector_admin_system_config_changed_section_webinterpret_connector>
                </observers>
            </admin_system_config_changed_section_webinterpret_connector>
            <adminhtml_init_system_config>
                <observers>
                    <webinterpret_adminhtml_init_system_config>
                        <type>singleton</type>
                        <class>Webinterpret_Connector_Model_Observer</class>
                        <method>hookIntoAdminhtmlInitSystemConfig</method>
                    </webinterpret_adminhtml_init_system_config>
                </observers>
            </adminhtml_init_system_config>
            <webinterpret_connector_notifications_before>
                <observers>
                    <webinterpret_connector_notifications_before>
                        <type>singleton</type>
                        <class>Webinterpret_Connector_Model_Observer</class>
                        <method>hookIntoWebinterpretConnectorNotificationsBefore</method>
                    </webinterpret_connector_notifications_before>
                </observers>
            </webinterpret_connector_notifications_before>
        </events>
        <request>
            <direct_front_name>
                <webinterpret_connector/>
            </direct_front_name>
        </request>
    </global>
    <frontend>
        <routers>
            <webinterpret_connector>
                <use>standard</use>
                <args>
                    <module>Webinterpret_Connector</module>
                    <frontName>webinterpret_connector</frontName>
                </args>
            </webinterpret_connector>
        </routers>
        <layout>
            <updates>
                <webinterpret_connector>
                    <file>webinterpret/connector.xml</file>
                </webinterpret_connector>
            </updates>
        </layout>
    </frontend>
    <adminhtml>
        <layout>
            <updates>
                <webinterpret_connector>
                    <file>webinterpret/connector.xml</file>
                </webinterpret_connector>
            </updates>
        </layout>
    </adminhtml>
    <default>
        <webinterpret_connector>
            <enabled>1</enabled>
            <key/>
            <file_repository>https://webstores.webinterpret.com</file_repository>
            <footer_enabled>1</footer_enabled>
            <footer/>
            <tracking_code/>
            <plugin_id/>
            <head/>
            <installation_mode>1</installation_mode>
            <global_notifications_enabled>1</global_notifications_enabled>
            <automatic_updates_enabled>1</automatic_updates_enabled>
